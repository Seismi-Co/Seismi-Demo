{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://seismi-co.github.io/Seismi-Demo",
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build": "bun build ./src/index.html --outdir=docs --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' && cp -r static/* docs/",
    "start": "NODE_ENV=production bun src/index.ts",
    "deploy": "bun run build"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19",
    "recharts": "^3.4.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-bluetooth": "^0.0.21"
  }
}
